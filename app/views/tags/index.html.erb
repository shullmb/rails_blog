<h1> Calling all tags</h1>

<ul>
	<% @tags.each do |tag| %>
		<li>
			<%= link_to tag.name, tag_path(tag) %>
			<%= link_to "Delete", tag_path(tag), method: :delete, data: {confirm: "Are you sure that you want to delete #{tag.name}?"}%>
		</li>
		
	<% end %>
</ul>
<br />
<%= link_to "<< Back to Article List", articles_path %>
